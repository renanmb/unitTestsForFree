{
  "name": "unit-tests-for-free",
  "displayName": "Unit_tests_for_free",
  "description": "Automate generation of unit tests for any language with one click allowing you to pursue to finer pleasures of life :) ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.1"
  },
  "extensions": [
    {
      "id": "unit-tests-for-free",
      "extensionDependencies": [
        "other-extension-id"
      ]
    }
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "unit-tests-for-free.generateUnitTest",
        "title": "Generate Unit Test"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p src",
    "clean": "rimraf out",
    "watch": "tsc -watch -p src",
    "lint": "eslint src --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --config webpack.config.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.75.1",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "openai": "^3.1.0"
  }
}
